{
  "name": "fit-on-a-floppy",
  "version": "0.0.1",
  "dependencies": {
    "jquery": "~3.4.1",
    "normalize.css": "~8.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.0.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/register": "~7.4.4",
    "acorn": "~6.2.0",
    "autoprefixer": "~9.6.1",
    "babel-eslint": "~9.0.0",
    "babelify": "~10.0.0",
    "browser-sync": "~2.26.7",
    "browserify": "~16.3.0",
    "del": "~2.2.2",
    "envify": "~4.1.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "~6.0.0",
    "foldero": "~0.1.1",
    "glob": "~7.1.4",
    "gulp": "~4.0.2",
    "gulp-changed": "~4.0.0",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "~2.1.3",
    "gulp-data": "~1.3.1",
    "gulp-declare": "^0.3.0",
    "gulp-eslint": "~6.0.0",
    "gulp-filter": "6.0.0",
    "gulp-handlebars": "^5.0.2",
    "gulp-htmlmin": "~5.0.1",
    "gulp-if": "~2.0.2",
    "gulp-imagemin": "~6.0.0",
    "gulp-load-plugins": "~2.0.0",
    "gulp-nunjucks-html": "~2.0.0",
    "gulp-plumber": "~1.2.1",
    "gulp-postcss": "~8.0.0",
    "gulp-rename": "~1.4.0",
    "gulp-rev": "9.0.0",
    "gulp-rev-delete-original": "0.2.3",
    "gulp-rev-rewrite": "2.0.0",
    "gulp-sass": "~4.0.2",
    "gulp-sourcemaps": "~2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "~3.0.8",
    "gulp-wrap": "^0.15.0",
    "handlebars": "^4.2.0",
    "imagemin-pngquant": "~7.0.0",
    "imagemin-svgo": "~7.0.0",
    "jasmine-core": "3.4.0",
    "js-yaml": "~3.13.0",
    "karma": "~4.2.0",
    "karma-browserify": "~6.1.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-jasmine": "~2.0.1",
    "lodash": "~4.17.4",
    "minimist": "~1.2.0",
    "nunjucks": "~3.2.0",
    "prettier": "~1.16.4",
    "puppeteer": "1.19.0",
    "rimraf": "2.6.3",
    "vinyl-buffer": "~1.0.1",
    "vinyl-source-stream": "~2.0.0",
    "watchify": "~3.11.0 "
  },
  "scripts": {
    "clean-deps": "npx rimraf node_modules",
    "clean": "npx gulp clean",
    "lint": "npx gulp eslint",
    "test": "npx gulp test",
    "format": "npx prettier --single-quote --write src/**/*.js",
    "serve": "NODE_ENV=development npx gulp serve",
    "serve:prod": "NODE_ENV=production npx gulp serve --production",
    "build": "gulp handlebars && NODE_ENV=production npx gulp --production",
    "deploy": "s3-deploy './build/**' --cwd './build/' --region=us-west-2 --bucket=fitonafloppy.website --deleteRemoved --gzip=xml,html,htm,js,css,ttf,otf,svg,txt"
  },
  "engines": {
    "node": ">=8.16.0"
  },
  "browserslist": [
    "last 2 version",
    "> 5%",
    "safari 5",
    "ios 6",
    "android 4"
  ],
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "templates": "_templates",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}",
      "handlebars": "**.hbs"
    }
  }
}
